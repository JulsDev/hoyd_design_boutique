    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "i18next": "^19.9.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.2.1",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.15.5",
    "react-i18next": "^11.8.10",
    "react-router-dom": "^5.2.0",
